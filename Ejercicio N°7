public class Main {


   // Método que recibe un número y devuelve su cuadrado
   public static int cuadrado(int numero) {
       return numero * numero;
   }


   public static void main(String[] args) {
       int resultado1 = cuadrado(5);


       System.out.println("El cuadrado de 5 es: " + resultado1);
   }
}
